                <script type="text/javascript">
                    function positive() {
                        document.getElementById("ContactForm_PositiveFeedback").setAttribute('value', 'true');
                        document.getElementById("feedbackForm").submit();
                        if (window.location.href.indexOf(".no/en/") > -1) {
                            alert("Thanks for your feedback! \nBest regards, Norwegian Institute of Public Health");
                        } else {
                            alert("Takk for tilbakemelding.\nHilsen Folkehelseinstituttet");
                        }
                    };
                    function negative() {
                        document.getElementById('feedback_text').style.display = "block";
                        document.getElementById('submitFormButton').style.display = "block";
                    };
                    function sendingfeedback() {
                        document.getElementById('feedback-container').style.display = "none";
                    };
                    function onFeedbackSubmit(token) {
                        document.getElementById("feedbackForm").submit();
                    };
                </script>
