<script type="text/javascript">
    function googleTranslateElementInit() {
        new google.translate.TranslateElement({ pageLanguage: 'en', multilanguagePage: true, gaTrack: true, gaId: 'UA-33935967-1' }, 'google_translate_element');
    }
</script>
<script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>
<script type="text/javascript">
    function setCurrentGroup(newGroupId) {
        $.ajax({
            url: '/ITG.TeamFusion/Account/SetCurrentGroup',
            type: 'POST',
            data: addAntiForgeryToken({ groupId: newGroupId }),
            success: function (d) {
                if (d.Success) {
                    $('#CurrentGroupName').text(d.GroupName);
                    $.growl({ message: 'Your current group is "' + d.GroupName + '"', icon: 'fa fa-info-circle' });
                }
                else {
                    $.growl({ message: 'The operation was not completed due to the following condition: ' + d.Message, icon: 'fa fa-warning' }, { type: 'danger' });
                }
            },
        });
    }
    $(function () {
        $('head').append('<meta name="google-translate-customization" content="e2277b06960b4272-2eb6999fb2b1a098-gdd12d87f933dec1d-9">');
        $('#PageTranslate ul.dropdown-menu').click(function (e) {
            e.cancelBubble = true;
            if (e.stopPropagation) {
                e.stopPropagation();
            }
        });

        $('#MainMenu li.dropdown').on('mouseenter', function () {
            if (viewportIs('md') || viewportIs('lg')) {
                if (!$(this).find('ul.dropdown-menu').is(':visible')) {
                    $(this).find('a.dropdown-toggle').dropdown('toggle');
                }
            }
        });
        $('#MainMenu li.dropdown').on('mouseleave', function () {
            if (viewportIs('md') || viewportIs('lg')) {
                if ($(this).find('ul.dropdown-menu').is(':visible')) {
                    $(this).find('a.dropdown-toggle').dropdown('toggle');
                }
            }
        });
        $('#MainMenu a.dropdown-toggle').on('click', function () {
            if (viewportIs('md') || viewportIs('lg')) {
                var href = $(this).attr('href');
                if (href != '#' && href.indexOf('javascript:') < 0) {
                    window.location.assign(href);
                }
            }
        });

        var checkboxLabelIdentifier = 0;

        $('.css-treeview').on('click', 'a', function (e) {
            if ($(this).attr('disabled') != undefined) {
                e.preventDefault();
            }
        });

        function addTreeElements(root, data) {
            $.each(data, function (index, element) {
                var $li = $('<li>');
                $li.addClass(element.addClass);
                if (element.children != null && element.children.length > 0) {
                    var checkboxId = 'checkbox-' + checkboxLabelIdentifier++;
                    var $checkbox = $('<input type="checkbox" id="' + checkboxId + '">');
                    $li.append($checkbox);
                    var $label = $('<label for="' + checkboxId + '">');
                    $li.append($label);
                }
                $li.append($('<span class="tree-node-icon">'));
                var $a = $('<a>');
                $a.text(element.title);
                if (typeof element.folderId == 'undefined' || !element.folderId) {
                    $a.attr('href', 'javascript:void(0)');
                    $a.attr('disabled', 'disabled');
                }
                else {
                    $a.attr('href', '/Discover?folderId=' + element.folderId);
                }
                $li.append($a);
                if (element.children != null && element.children.length > 0) {
                    var $ul = $('<ul>');
                    addTreeElements($ul, element.children);
                    $li.append($ul);
                }
                root.append($li);
            });
        }

        $('#UserBookmarks').on('show.bs.dropdown', function (e) {
            var $this = $(this);
            if ($this.hasClass('data-loaded')) {
                return;
            }

            var $toggle = $(e.relatedTarget);
            var $that = $this.find('ul.dropdown-menu');

            $.ajax({
                url: '/ITG.TeamFusion/Bookmarks/GetChildrenFolders?parent=0',
                type: 'GET',
                success: function (d) {
                    if (d != null && d.length > 0) {
                        var $div = $('<div class="css-treeview">');
                        var $ul = $('<ul>');
                        addTreeElements($ul, d);
                        $div.append($ul);
                        $that.append($div);
                        $this.addClass('data-loaded');

                        $that.click(function (e) {
                            e.cancelBubble = true;
                            if (e.stopPropagation) {
                                e.stopPropagation();
                            }
                        });
                    }
                    else if (typeof d.Success != 'undefined' && !d.Success) {
                        $.growl({ message: 'The operation was not completed due to the following condition: ' + d.Message, icon: 'fa fa-warning' }, { type: 'danger' });
                    }
                    else {
                        $.growl({ message: 'General failure', icon: 'fa fa-warning' }, { type: 'danger' });
                    }
                },
                error: function () {
                    $.growl({ message: 'The operation was not completed due to an unexpected server response.', icon: 'fa fa-warning' }, { type: 'danger' });
                },
                complete: function () {
                    $that
                        .find('.operation-pending')
                        .parent()
                        .remove();
                }
            });
        });

        $('#ChangeCurrentGroupSet').click(function () {
            setCurrentGroup($('#ChangeCurrentGroupList').val());
        });

        $('#ChangeCurrentGroupDialog').on('show', function (e) {
            $('#ChangeCurrentGroupList').empty();

            $.ajax({
                url: '/ITG.TeamFusion/Account/GetMemberGroups',
                type: 'GET',
                success: function (d) {
                    if (d.Success) {
                        $.each(d.Groups, function (index, element) {
                            var $option = $('<option>');
                            $option.val(element.Id);
                            $option.text(element.Name);
                            if (element.IsCurrent)
                                $option.prop('selected', 'selected');
                            $('#ChangeCurrentGroupList').append($option);
                        });
                    }
                    else {
                        $.growl({ message: 'The operation was not completed due to the following condition: ' + d.Message, icon: 'fa fa-warning' }, { type: 'danger' });
                    }
                },
                error: function () {
                    $.growl({ message: 'The operation was not completed due to an unexpected server response.', icon: 'fa fa-warning' }, { type: 'danger' });
                },
                complete: function () {
                }
            });
        });
    });
</script>

