    <script type="text/javascript">
    var allFolders = [] || [];
    function GetAssetAttachmentFolders(projectID) {
        if (!projectID)
            return allFolders
        else
            return _.where(allFolders, { AssetFk: projectID });
    }
    function SetAssetAttachmentFolders(projectID, folders) {
        allFolders = _.filter(allFolders, function(folder){ return folder.AssetFk != projectID; })
        _.each(folders, function(folder){
            allFolders.push(folder);
        });
    }

    var _initialFileuploadDocsURL = $('#fileupload-docs').attr('action');
    $('#fileupload-docs').attr(
        'action',
        _initialFileuploadDocsURL + '&defaultVisibility=' + $('#default-attachment-visibility').val());

    function folderSelector(callback, attachmentName) {
        var that = storageSelector.builtInStorage;
        that.folderSelector.show(that.folders,
            {
                validationFn: function(selectedFolder){
                    if (storageSelector.builtInStorage.getFile(attachmentName, selectedFolder.ClientID))
                        return 'A file with the same name already exists in the selected folder';
                }
            });
        $(that.folderSelector).off('folderSelected');
        $(that.folderSelector).on('folderSelected', _.bind(function (ev, newParentFolder) {
            callback(newParentFolder);
        }, that));
    }

    function shareAttachment(urlTemplate, attachmentID){
        var url = urlTemplate.replace('%7BattachmentID%7D', attachmentID);
        $('.shareAttachmentDialogInput').val(url);
        $('#shareAttachmentDialog').modal('show');
        $('.shareAttachmentDialogInput').select();
        setTimeout(function(){
            $('.shareAttachmentDialogInput').focus();
        }, 1000);
    }

    function copyAttachment(attachmentName, type, folderID) {
        folderSelector(function (targetFolder) {
            $.ajax('/ITG.TeamFusion/AssetAttachments/CopyAttachment', {
                type: 'POST',
                dataType: 'json',
                data: addAntiForgeryToken({
                    assetId: window.projectID,
                    fileName: attachmentName,
                    folderID: folderID,
                    targetFolderID: targetFolder.Id
                }),
                success: function (d) {
                    if (d.Success) {
                        storageSelector.builtInStorage.loadAttachments();
                    }
                    else {
                        $.growl({ message: 'The operation was not completed. ' + d.Message, icon: 'fa fa-warning' }, { type: 'danger' });
                    }
                }
            });
        }, attachmentName);
    }

    function moveAttachment(attachmentName, type, folderID) {
        folderSelector(function (targetFolder) {
            $.ajax('/ITG.TeamFusion/AssetAttachments/MoveAttachment', {
                type: 'POST',
                dataType: 'json',
                data: addAntiForgeryToken({
                    assetId: window.projectID,
                    fileName: attachmentName,
                    folderID: folderID,
                    targetFolderID: targetFolder.Id
                }),
                success: function (d) {
                    if (d.Success) {
                        storageSelector.builtInStorage.loadAttachments();
                    }
                    else {
                        $.growl({ message: 'The operation was not completed. ' + d.Message, icon: 'fa fa-warning' }, { type: 'danger' });
                    }
                }
            });
        }, attachmentName);
    }

    function showAttachmentHistory(attachmentName, type, folderID, assetID) {
        if (type == 0) {
            $('.history-modal .loading').css({ 'display': 'table' });
            $('.history-modal .history-text').text('');
            $('.history-modal').modal('show');
            $.ajax('/ITG.TeamFusion/AssetAttachments/AttachmentHistory', {
                type: 'POST',
                dataType: 'json',
                data: addAntiForgeryToken({
                    folderID: folderID,
                    assetId: assetID || window.projectID,
                    attachmentName: attachmentName,
                    folderID: folderID
                }),
                success: function (result) {
                    $('.history-modal .loading').css({ 'display': 'none' });
                    if (result.history != null && result.history.length > 0) {
                        $('.history-modal .history-text').html(result.history);
                    } else {
                        $('.history-modal .history-text').html('<p>No history is available for this file</p>');
                    }
                },
                error: function (qXHR, textStatus, errorThrown) {
                    $('.history-modal .loading').css({ 'display': 'none' });
                    $('.history-modal .history-text').html('<p class="error">' + textStatus + '</p><p class="error">' + errorThrown + '</p></p>');
                }
            });
        } else {
            var url =
                '/ITG.TeamFusion/AssetAttachments/AttachmentHistory?assetId=' + window.projectID + '&folderID=' + folderID + '&attachmentName=' + escape(attachmentName);
            window.open(url, '_blank');
        }

        return false;
    }

    function showCreateNewDocument() {
        $('#fileupload-docs-document-title').removeAttr('required');
        $('#fileupload-docs-document-error').hide();
        $('#fileupload-docs-document-title').val('');
        $('.newdoc-modal').modal('show');
        return false;
    }

    // is called when user presses 'Create' in 'Create New Document' modal dlg
    function createNewDocument() {
        $('#fileupload-docs-document-error').hide();

        var newDocumentTitle = $('#fileupload-docs-document-title').val();
        newDocumentTitle = $.trim(newDocumentTitle);
        if (newDocumentTitle.length == 0) {
            $('#fileupload-docs-document-title').attr('required', 'required');
        } else {
            $('#fileupload-docs-document-title').removeAttr('required');
            $.ajax({
                url: '/ITG.TeamFusion/AssetAttachments/NewDocument' + '?assetId=' + window.projectID,
                data: addAntiForgeryToken({
                    newDocumentTitle: newDocumentTitle,
                    visibility: $('#default-attachment-visibility').val(),
                    folderID: storageSelector.builtInStorage.getCurrentFolderID()
                }),
                type: 'POST',
                success: function (d) {
                    if (d.Success) {
                        $('.newdoc-modal').modal('hide');
                        storageSelector.loadAttachments();
                        window.open(d.DocumentURL, '_blank');
                    } else {
                        $('#fileupload-docs-document-title').attr('required', 'required');
                        $('#fileupload-docs-document-error').text(d.Message).show();
                    }
                }
            });
        }
        return false;
    }

    function saveSort(sort) {
        $.cookie('attachments-sort', sort);
        updateSortButton(sort);
    }

    function loadSort() {
        var sort = $.cookie('attachments-sort') || 'sort-name-asc';
        if (sort) {
            $('#asset-attachments a#' + sort).trigger('click');
        }
    }

    function updateSortButton(sort) {
        if (sort) {
            $('#asset-attachments #sort-button .cur-sort').text($('#asset-attachments #' + sort).text());
        }
    }

    // onclick handler
    function sort(event) {
        var attr = null;
        var order = null;
        var isNum = false;
        switch (event.target.id) {
            case 'sort-name-asc':
                attr = 'attachment-name';
                order = 'asc';
                break;
            case 'sort-name-desc':
                attr = 'attachment-name';
                order = 'desc';
                break;
            case 'sort-size-asc':
                attr = 'attachment-size';
                isNum = true;
                order = 'asc';
                break;
            case 'sort-date-asc':
                attr = 'attachment-date';
                isNum = true;
                order = 'asc';
                break;
            case 'sort-type-asc':
                attr = 'attachment-type';
                order = 'asc';
                break;
            case 'sort-visibility-asc':
                attr = 'attachment-visibility';
                isNum = true;
                order = 'asc';
                break;
        }

        $('.files').jSort({
            sort_by: 'div.media-body a',
            item: 'div.media',
            order: order,
            sort_by_attr: true,
            attr_name: attr,
            is_num: isNum
        });
        saveSort(event.target.id);
        return false;
    }

    function toggleVisibility(event) {
        var $el = $(event.target).parent().parent().find('[attachment-id]');
            
            var securityUrl = $(event.target).attr('security-url');        
            if (securityUrl) {
                var item = $(event.target).parents('div.template-download');
                var newAttachmentVisiblity = parseInt(item.find('a[attachment-visibility]').attr('attachment-visibility'));
                newAttachmentVisiblity ^= 1;

                $.ajax(securityUrl, {
                    type: 'POST',
                    data: addAntiForgeryToken({ visibility: newAttachmentVisiblity }),
                    success: function (d) {
                        var perror = item.find('p.error');

                        if (d.Success) {
                            var svisible = item.find('span.attachment-visibility');
                            perror.html('');
                            svisible.text(d.Visibility ? 'Private' : 'Restricted');
                            $(event.target).text(d.Visibility ? 'Make Restricted' : 'Make Private');
                            item.find('a[attachment-visibility]').attr('attachment-visibility', d.Visibility);
                        } else {
                            perror.html('<b>Error:</b> ' + d.Message);
                        }
                    }
                });
            }
        
        storageSelector.builtInStorage.fileChanged({
            name: $el.attr('attachment-name'),
            folder_id: $el.attr('attachment-folder-id'),
            folder_client_id: $el.attr('attachment-folder-client-id'),
            security: newAttachmentVisiblity
        });
    }

    $(function () {
        
        if (typeof window.tmpl != 'undefined' && typeof window.tmpl.cache != 'undefined')
            window.tmpl.cache = {};

        $('#fileupload-docs').fileupload({
            limitConcurrentUploads: 3,
            uploadTemplateId: 'template-upload-docs',
            downloadTemplateId: 'template-download-docs',
            dropZone: $('#fileupload-docs'),
            autoUpload: true,
            send: function (e, data) {
                for (var index = 0; index < data.files.length; index++) {
                    $('#fileupload-docs a[attachment-name]').filter(function () {
                        return $(this).attr('attachment-name') == data.files[index].name;
                    }).parents('div.template-download').remove();
                }
                return true;
            },
            
            destroy: function (e, data) {
                var item = data.context;
                var that = $(this).data('fileupload');
                if (data.url) {
                    confirmModal("Confirm Deletion", "<p>This will permanently delete the file from the repository.</p><p>Would you like to continue?</p>",
                        function () {
                            $.ajax(data)
                                .success(function (d) {
                                    $('#fileupload-docs').trigger('fileDeleted', data);
                                    if (d.Success) {
                                        that._adjustMaxNumberOfFiles(1);
                                        item.fadeOut(function () {
                                            item.remove();
                                        });
                                    } else {
                                        item.find('p.error')
                                            .html('<b>Error:</b> ' + d.Message);
                                    }
                                });
                        });
                } else {
                    item.fadeOut(function () {
                        item.remove();
                    });
                }
            },
            
        });

        $('#fileupload-docs').data('fileupload')._formatFileSize = function (bytes) {
            if (typeof bytes !== 'number') {
                return '';
            }
            if (bytes >= 1073741824) {
                return (bytes / 1073741824).toFixed(0) + ' GB';
            }
            if (bytes >= 1048576) {
                return (bytes / 1048576).toFixed(0) + ' MB';
            }
            if (bytes >= 1024) {
                return (bytes / 1024).toFixed(0) + ' KB';
            }
            return bytes + ' B';
        };

        $('#fileupload-docs').delegate(
            'a.delete',
            'click',
            {
                fileupload: $('#fileupload-docs').data('fileupload')
            },
            function () {
                if ($(this).hasClass('attachment-deleteButton'))
                    return $('#fileupload-docs').data('fileupload')._deleteHandler.apply(this, arguments);
            });
    });
</script>    
 <script type="text/javascript">
         
    window.projectID = 3819;
    $('#fileupload-docs')[0].action = UpdateQueryString('assetID', window.projectID, $('#fileupload-docs')[0].action);
    $(function () {
        showStorageSelector(3819, {"StorageType":"BuiltIn","GoogleDriveShareUrl":null,"GoogleGroupUrl":null,"GoogleCalendarUrl":null,"OneDriveUrl":null,"OneDriveUserFk":null,"Id":1359});
    });
    
    function showStorageSelector(projectID, projectDetail){
        window.storageSelector = _.isUndefined(window.storageSelector) ? new StorageSelector() : window.storageSelector;
        storageSelector.projectDetail = projectDetail;
        storageSelector.init({
            general:{
                projectID: projectID,
                urls:{
                    authorization: '/ProjectStorage/Authorization?storageType=20',
                    getStorageItems: '/ProjectStorage/GetStorageItems?storageType=20',
                    checkToken: '/ProjectStorage/CheckToken?storageType=20',
                    iconFolder: '/Modules/ITG.TeamFusion/Content/images/ArtifactTypes/',
                    loadingImage: '/Modules/ITG.TeamFusion/Content/images/ajax-loader.gif'
                }
            },
            builtIn:{
                urls:{
                    iconFolder: '/Modules/ITG.TeamFusion/Content/images/ArtifactTypes/',
                    attachmentFoldersController: '/ITG.TeamFusion/AssetAttachmentFolders/ActionName?assetID=%7BassetID%7D'.replace('%7BassetID%7D', projectID)
                },
                editMode: 'inline'
            }
        });
        storageSelector.show();
    };

    function UpdateQueryString(key, value, url) {
        if (!url) url = window.location.href;
        var re = new RegExp("([?&])" + key + "=.*?(&|#|$)(.*)", "gi"),
            hash;

        if (re.test(url)) {
            if (typeof value !== 'undefined' && value !== null)
                return url.replace(re, '$1' + key + "=" + value + '$2$3');
            else {
                hash = url.split('#');
                url = hash[0].replace(re, '$1$3').replace(/(&|\?)$/, '');
                if (typeof hash[1] !== 'undefined' && hash[1] !== null)
                    url += '#' + hash[1];
                return url;
            }
        }
        else {
            if (typeof value !== 'undefined' && value !== null) {
                var separator = url.indexOf('?') !== -1 ? '&' : '?';
                hash = url.split('#');
                url = hash[0] + separator + key + '=' + value;
                if (typeof hash[1] !== 'undefined' && hash[1] !== null)
                    url += '#' + hash[1];
                return url;
            }
            else
                return url;
        }
    }
</script>



	</div>
</article>

