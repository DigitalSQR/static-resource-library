                            <script type="text/javascript">
                                function Follow() {
                                    $.get(
                                        '/ITG.TeamFusion/AssetCommunityFeedback/UserLikeIt',
                                        { assetId: 3819, feedbackType: 'Follow' },
                                        function (data) {
                                            $('#follow-asset-button')
                                                .text('Following')
                                                .attr('following', 'yes')
                                                .removeClass('btn-default')
                                                .addClass('btn-success');
                                        });
                                }

                                function Unfollow() {
                                    $.get(
                                        '/ITG.TeamFusion/AssetCommunityFeedback/UserDoesNotLikeIt',
                                        { assetId: 3819, feedbackType: 'Follow' },
                                        function (data) {
                                            $('#follow-asset-button')
                                                .text('Follow')
                                                .attr('following', 'no')
                                                .removeClass('btn-success')
                                                .addClass('btn-default');
                                        });
                                }

                                function ToggleFollow() {
                                    if ($('#follow-asset-button').attr('following') == 'yes') {
                                        Unfollow();
                                    }
                                    else {
                                        Follow();
                                    }
                                }

                                $(function () {
                                    if ($.cookie('tf-action') == 'follow') {
                                        $.removeCookie('tf-action');
                                        Follow();
                                    }

                                    $('#follow-asset-button').click(function() {
                                        ScreenActionAuthenticationRequired('follow');
                                    });
                                });
                            </script>
