<script type="text/javascript">

/*
Utilites for dealing with links and cookies.
*/
$("[data-action='set-cookie-from-query-string']").each(function(index, item) {
  //console.log(this)
  var paramName = $(this).data('param-name');
  var cookieName = $(this).data('cookie-name');
  var search = window.location.search;
  var paramInSearchString = search.indexOf('?' + paramName + '=');
  //console.log([paramName, cookieName, search, paramInSearchString]);
  if (paramInSearchString > -1){
    var paramValue = search.substr(paramName.length + 2, search.length);
    //console.log('setting cookie to be' [cookieName, paramValue]);
    $.cookie(cookieName, paramValue);
  }});

</script>

