                    <p class="error"><b>Error:</b> {%=file.error%}</p>
                </div>
                {% } else { %}
                <a class="pull-left hidden-xs" href="{%=file.url%}" title="{%=file.name%}" rel="nofollow" target="_blank">
                    <img class="media-object" src="{%=file.thumbnail_url%}" width="96" height="96" />
                </a>
                <div class="media-body">
                    <h4 class="media-heading">
                        <a href="{%=file.url%}" title="{%=file.name%}"
                           attachment-name="{%=file.name%}"
                           attachment-folder-client-id="{%=file.folder_client_id%}"
                           attachment-folder-id="{%=file.folder_id%}"
                           attachment-id="{%=file.attachment_id%}"
                           attachment-size="{%=file.size%}"
                           attachment-date="{%= file.milliseconds_since_modified ? file.milliseconds_since_modified : Number.MAX_VALUE %}"
                           attachment-type="{%=file.thumbnail_url%}"
                           attachment-visibility="{%=file.security.toString()%}"
                           rel="nofollow" target="_blank">{%=file.name%}</a>
                    </h4>
                    <p>{%=file.time_since_modified ? 'Updated ' + file.time_since_modified + ', ':''%}<span class="attachment-visibility">{% if (file.security == 0 || file.security == -1) { %}Restricted{% } else { %}Private{% } %}</span>, {%=o.formatFileSize(file.size)%}</p>
                    <p>
                        {% if (file.error) { %}
                        <button type="button" class="btn btn-primary btn-sm"
                                data-type="{%=file.delete_type%}"
                                data-url="{%=file.delete_url%}"
                                title="Delete the attachment">
                            Delete
                        </button>
                        {% } else { %}
                        <a class="btn btn-primary btn-sm" href="{%=file.url%}" title="Open file" target="_blank">Open</a>
                                                    
                                {% if (file.type == 0) { %}
                                    <a class="btn btn-default btn-sm attachment-shareButton" data-attachment-id="{%=file.id%}" href="javascript:void(0);" title="Share this Resource" onclick="return shareAttachment('https://aehin.hingx.org/ProjectStorage/DownloadAttachment?attachmentID=%7BattachmentID%7D', '{%=file.id%}')">Share</a>
                                {% } %}
                            
                            <a class="btn btn-default btn-sm attachment-describeAsResourceButton" data-attachment-id="{%=file.id%}" href="/ITG.TeamFusion/AssetAttachments/NewAsTemplate?assetId={%=file.project_id%}&describeAttachment={%=file.name%}&folderID={%=file.folder_id%}" title="Describe as a Resource">Describe as a Resource</a>
                        {% } %}
                        <comments-widget params="{ objectType: portalContext.objectType.assetAttachment, objectID: {%=file.id%}, count: {%=file.comment_count%}, likeRateSum: {%=file.like_rate_sum%}, isLiked: {%=file.like_is_liked%}, btnClass: 'btn-sm', marginLeft: '0' }"></comments-widget>
                    </p>
                    <p class="error"></p>
                </div>
                <comments-widget-body params="{ objectType: portalContext.objectType.assetAttachment, objectID: {%=file.id%}, count: {%=file.comment_count%}, likeRateSum: {%=file.like_rate_sum%}, isLiked: {%=file.like_is_liked%}, btnClass: 'btn-sm', marginLeft: '0' }"></comments-widget-body>
                {% } %}                
            </div>            
            {% } %}
        </script>
