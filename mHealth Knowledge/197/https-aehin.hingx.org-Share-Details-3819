        <script id="template-download-docs" type="text/x-tmpl">
            {% for (var i=0, file; file=o.files[i]; i++) { %}
            <div class="media template-download ffade">
                {% if (file.error) { %}
                <a class="pull-left hidden-xs" href="javascript:void(0);">
                    <img class="media-object" src="" alt="" width="96" height="96">
                </a>
