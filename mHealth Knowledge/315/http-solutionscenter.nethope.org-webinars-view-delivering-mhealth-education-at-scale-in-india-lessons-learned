<script>

$(document).ready(function() {
  var webinars = !$('a[data-channel="webinars"]').hasClass( "unselectedButton" );
  var events = !$('a[data-channel="events"]').hasClass( "unselectedButton" );
  var channelP='####';
  if(webinars && events ){
    var channelP = 'webinars|events';
  }
  else if(webinars){
    var channelP = 'webinars';
  }
  else if(events){
    var channelP = 'webinars';
  }
  //Webinar, Events Calender View
  $('#calendar').fullCalendar({
    header: {
      left: 'prev,next today',
      center: 'title',
      right: 'month,agendaWeek,agendaDay'
    },
    defaultDate: '2019-09-19',
    editable: true,
    eventLimit: true, // allow "more" link when too many events
    events: {
      url: '/webinars/calendar-json',
      error: function() {$('#script-warning').show();},
      data: {
        channelp: channelP,
      }
    },
    loading: function(bool) {$('#loading').toggle(bool);}
  });

  $('.view-selector').on('click', function(e){

      $(this).parent().find('.btn').removeClass('active');
    e.preventDefault();
    var viewId = $(this).attr("href");
    //console.log(viewId);
    $('.single-view').css('display','none');
    $(viewId).css('display','block');

    $('#calendar').fullCalendar('destroy');
    RenderCalendar();


    $(this).addClass('active');

    //$('#calendar').fullCalendar('today');
  });

  $('#channel_toggle').on('click','.button', function(e){
    e.preventDefault();
    $('#calendar').fullCalendar('destroy');
    RenderCalendar();
    //console.log('sajjad');
  });

  //Log the page loads
  var current_href = jQuery(location).attr('href');
  var current_title = document.title;
  logClick('<strong>Page Views:</strong> '+current_title, current_href);

});

//Load Calender View for Webinar, Events
function RenderCalendar(){
  var webinars = !$('a[data-channel="webinars"]').hasClass( "unselectedButton" );
  var events = !$('a[data-channel="events"]').hasClass( "unselectedButton" );
  var channelP='####';
  if(webinars && events ){
    var channelP = 'webinars|events';
  }
  else if(webinars){
    var channelP = 'webinars';
  }
  else if(events){
    var channelP = 'webinars';
  }

  $('#calendar').fullCalendar({
    header: {
      left: 'prev',
      center: 'title',
      right: 'next'
    },
    defaultDate: '2019-09-19',
    editable: true,
    eventLimit: true, // allow "more" link when too many events
    events: {
      url: 'webinars/calendar-json',
      error: function() {$('#script-warning').show();},
      data: {
        channelp: channelP,
      }
    },
    loading: function(bool) {$('#loading').toggle(bool);}
  });
}

</script>

</body>
</html>


