    function ScreenActionAuthenticationRequired(action) {
        $.cookie('tf-action', action);
        $('#screen-actions-auth-dialog').modal('show');
        return false;
    }

    function addAntiForgeryToken(data, stringMode) {
        var antiForgeryToken = getAntiForgeryToken();
        if (!stringMode) {
            if (!data)
                data = {};
            data.__RequestVerificationToken = antiForgeryToken;
        } else {
            if (data)
                data += '&';
            data += '__RequestVerificationToken=' + antiForgeryToken;
        }
        return data;
    }

    function getAntiForgeryToken() {
        return $('input[name=__RequestVerificationToken]').val();
    }

    $(function () {
        $('#screen-actions-auth-dialog').on('hide', function (e) {
            $.removeCookie('tf-action');
        });
        $('#screen-actions-auth-login').click(function () {
            window.location.href ='/Users/Account/LogOn?ReturnUrl=LOGIN-URL'
                .replace(/LOGIN-URL/, encodeURIComponent(window.location.href));
        });
        $('#screen-actions-auth-register').click(function () {
            window.location.href = '/Users/Account/Register?ReturnUrl=LOGIN-URL'
                .replace(/LOGIN-URL/, encodeURIComponent(window.location.href));
        });

        $('#MenuV').on('click', 'a', function (event) {
            var link = $(event.currentTarget).attr('href');
            if (link && link.length > 0 && link[0] == '#') {
                $(document).trigger('navMenuClicked', link);
                $.scrollTo(link, 300);
                return false;
            }
            return true;
        });
    });
</script>

