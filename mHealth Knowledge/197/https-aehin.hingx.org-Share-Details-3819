                You are not participating on any project.
            </div>
            <!-- /ko -->
            <!-- /ko -->
        </div>
        <div style="width: 100%;height:420px;padding: 5px;display:none;color:red;" data-bind="visible: isError">
            <div style="text-align: center; position: relative; top: 40%;" data-bind="text: errorText"></div>
        </div>
    </div>
</div>
<script type="text/javascript">
    function LinkToProjects() {
        var self = this;
        self.data = ko.observable(null);
        self.isDataLoading = ko.observable(false);
        self.isError = ko.observable(false);
        self.errorText = ko.observable();
        self.resourceId = ko.observable(null);

        self.showList = function (resourceId) {
            self.resourceId(resourceId);
            self.data(null);
            self.isError(false);
            self.isDataLoading(true);
            $('#link-to-projects-dialog').modal('show');
            $.ajax('/Share/LinkToProjects', {
                type: 'GET',
                data: {
                    resourceId: resourceId
                },
                success: function (data) {
                    if (data) {
                        self.data(ko.mapping.fromJS(data));
                        self.data().projects.sort(function(l, r) { return l.title() > r.title() ? 1 : -1; });
                    } else {
                        self.errorText('The specified resource ID is invalid!');
                        self.isError(true);
                    }
                    self.isDataLoading(false);
                },
                error: function () {
                    self.isDataLoading(false);
                    self.errorText('An error occurred while loading projects. Please, try again later.');
                    self.isError(true);
                }
            });
        }

        self.link = function (projectId) {
            $('#link-to-projects-dialog').modal('hide');
            $.ajax('/Share/LinkToProjects', {
                type: 'POST',
                data: addAntiForgeryToken({
                    resourceId: self.resourceId(),
                    projectId: projectId
                }),
                success: function (data) {
                    if (data.Success) {
                        $.growl({ message: data.Message, icon: 'fa fa-info-circle' });
                    } else {
                        $.growl({ message: 'The operation was not completed due to the following condition: ' + data.Message, icon: 'fa fa-warning' }, { type: 'danger' });
                    }
                }
            });
        };
    };
    var linkToProjects = null;

    $(function () {
        $.post('/Share/ResourceViewed', addAntiForgeryToken({ assetId: 3819 }));

        linkToProjects = new LinkToProjects();
        ko.applyBindings(linkToProjects, $('#link-to-projects-dialog').get(0));

        if (typeof String.prototype.startsWith != 'function') {
            String.prototype.startsWith = function (str) {
                return this.slice(0, str.length) == str;
            };
        }
        var cookieAction = $.cookie('tf-action');
        if (cookieAction && cookieAction.startsWith('link-to-project')) {
            $.removeCookie('tf-action');
            var assetId = parseInt(cookieAction.substring('link-to-project'.length));
            linkToProjects.showList(assetId);
        }

        UpdateLastViewCookie(false, 3819);
    });
</script>

	</div>
