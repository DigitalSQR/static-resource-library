    (function() {
        'use strict';
        var pageContextElement = document.getElementById('sv-scroll-page-context');
        if (pageContextElement) {
            var scrollContext = JSON.parse(pageContextElement.textContent);
            if (window.Scroll) {
                if (window.Scroll.Versions) {
                    window.Scroll.Versions.Context = scrollContext;
                } else {
                    window.Scroll.Versions = {Context: scrollContext};
