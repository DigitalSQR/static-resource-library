<script type="text/javascript">
    
    function initItemSlider() {
        $('#asset-visuals .panel-content .uploadVisual').detach().prependTo('#asset-visuals .files');
        $('#asset-visuals-slider').itemSlider({
            'prevPageSelector': '#asset-visuals-prev',
            'nextPageSelector': '#asset-visuals-next',
            items: '.sliderItem'
        });
    }

    window.onload = function () {
        if ($('#asset-visuals-slider').parents('article.widget-content').is(':visible')) {
            initItemSlider();
        }
    };
    
    function initZoombox() {
        var _zoomBox = $('a.zoombox').zoombox({
            theme: 'lightbox',        //available themes : zoombox,lightbox, prettyphoto, darkprettyphoto, simple
            opacity: 0.8,              // Black overlay opacity
            duration: 600,              // Animation duration
            animation: true,             // Do we have to animate the box ?
            width: 600,              // Default width
            height: 400,              // Default height
            gallery: true,             // Allow gallery thumb view
            autoplay: false                // Autoplay for video
        });
    }

    function refreshZoomBox() {
            
        $('#asset-visuals-slider').itemSlider('destroy');
        initItemSlider();
        

        $('#asset-visuals-slider-modal a.zoombox').remove();
        $.zoombox.clear();
        $('#asset-visuals .panel-content .uploadVisual').detach().prependTo('#asset-visuals .files');
        initZoombox();
    }

    function initVisuals() {
            initItemSlider();
        initZoombox();
    }

    $(function () {
        //if ($('#asset-visuals-slider').parents('article.widget-content').is(':visible'))
        {
            initZoombox();
        }
    });

    $(function () {

        $(document).on('click', '.visualDeleteButton', function (ev) {
            var visualID = $(ev.currentTarget).data('visual-id');
            confirmModal("Confirm Deletion", "<p>This picture will be permanently deleted.</p><p>Would you like to continue?</p>", function () {
                return $.ajax(
                  {
                      type: 'DELETE',
                      dataType: 'json',
                      contentType: 'application/json; charset=utf-8',
                      url: '/Share/AssetVisuals/3819',
                      data: JSON.stringify(addAntiForgeryToken({
                          assetID: '3819',
                          visualID: visualID
                      }))
                  }
                  , null, null, 'json')
                    .always(function (data) {
                        if (data.responseText == 'OK') {
                            $('#asset-visuals .sliderItem[data-visual-id=' + visualID + ']').remove();
                            $('#asset-visuals .panel-content .uploadVisual').detach().prependTo('#asset-visuals .files');

                            refreshZoomBox();
                        }
                        else {
                            $.growl({ message: 'Error occured, please try again later.', icon: 'fa fa-warning' }, { type: 'danger' });
                        }
                    });
            });
        });

        $(document).on('click', '.visualMakeDefaultButton', function (ev) {
            var visualID = $(ev.currentTarget).data('visual-id');
            $('#asset-visuals .sliderItem.defaultVisual').removeClass('defaultVisual');
            $('#asset-visuals .sliderItem[data-visual-id=' + visualID + ']').addClass('defaultVisual');
            return $.ajax(
              {
                  type: 'POST',
                  dataType: 'json',
                  contentType: 'application/json; charset=utf-8',
                  url: '/ITG.TeamFusion/AssetVisuals/UpdateAssetVisual',
                  data: JSON.stringify(addAntiForgeryToken({
                      assetID: '3819',
                      visualID: visualID,
                      isDefault: true
                  }))
              }
              , null, null, 'json')
                .fail(function (data) {
                    $.growl({ message: data.responseJSON.Message, icon: 'fa fa-warning' }, { type: 'danger' });
                });
        })
    });
</script>
