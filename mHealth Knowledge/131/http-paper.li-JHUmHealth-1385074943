  <script>
    $(document).ready(function(){
      if ($.browser.msie && parseInt($.browser.version) <= 8){
        $('#ie_banner')
          .find('.message').html(paperli.util.translate('message.ie_not_supported')).end()
          .show();
        
        $('#ie_banner .close').click(function(e){
          e && e.preventDefault();
          $('#ie_banner').slideUp();
        });
      };        
    });
