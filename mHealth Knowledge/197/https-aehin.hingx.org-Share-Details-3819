<script id="CommentsWidget-CommentsWidgetBodyTemplate" type="text/html"><span data-bind="attr: {'data-comments-key': commentsKey()}" class="comments">
  <div data-bind="visible: isExpanded" style="background-color: #f1f1f1; margin-top: 10px; padding: 0 20px 1px;">
    <div data-bind="visible: canWrite" class="comments-newCommentForm">
      <table>
        <tr>
          <td data-bind="validationOptions: { insertMessages: false }"  style="width: 100%;">
            <textarea data-bind="value: newComment, valueUpdate: 'keyup', enterkey: addComment, autosize: newComment" placeholder="Leave a comment..."></textarea>
          </td>
          <td>
            <div data-bind="click: addComment" class="btn btn-primary btn-sm comments-newCommentButton">Send</div>
          </td>
        </tr>
      </table>
    </div>
    <div data-bind="visible: !isLoading()" style="margin-top: 15px;">
      <div data-bind="foreach: comments">
        <div class="comments-commentCard">
          <a data-bind="attr: { href: portalContext.urls.getProfileUrl(userID())}" class="hidden-xs" style="float: left !important;">
            <img data-bind="attr: {src: portalContext.urls.getAvatarUrl(userID()) }" class="comments-avatar"></img>
